    <!-- 中国移动的插码开始 -->
    <script src="/common/webtrends.load.js"></script>
    <script>multiTrack();</script>
    <!-- 中国移动的插码结束 -->

    <!-- js逻辑文件开始 -->
    <script src="/dist/<%- path %>.js"></script>
    <!-- js逻辑文件结束 -->
