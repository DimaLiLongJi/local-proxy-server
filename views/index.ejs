    <noscript>
      <strong>We're sorry but js-template doesn't work properly without JavaScript enabled. Please enable it to continue.</strong>
    </noscript>
    <div id="app">
      <router-view></router-view>
    </div>

    <!-- 中国移动APP SDK 开始 -->
    <script src="/common/leadeon.js"></script>
